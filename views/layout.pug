block options
doctype html

html(lang='ko')
	head
		meta(charset='utf-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge, chrome=1')
		meta(name='viewport', content='width=device-width, initial-scale=1.0')
		title= title
		link(rel='icon', type='image/png' href='/favicon.png')
		
		meta(name='theme-color', content='#1B1C1D')
		meta(name='mobile-web-app-capable', content='yes')
		meta(name='apple-mobile-web-app-capable', content='yes')
		
		meta(property='og:type', content='website')
		meta(property='og:title', content='MineJet')
		meta(property='og:description', content='Powerful web app for MC:PE servers')
		
		meta(name='twitter:title', content='MineJet')
		meta(name='twitter:description', content='Powerful web app for MC:PE servers')
		
		link(rel='stylesheet', type='text/css', href='https://cdn.jsdelivr.net/semantic-ui/2.2.2/semantic.min.css')
		block stylesheets
	body
		block body
		script(src='https://code.jquery.com/jquery-3.1.0.min.js')
		script(src='https://cdn.jsdelivr.net/semantic-ui/2.2.2/semantic.min.js')
		block scripts